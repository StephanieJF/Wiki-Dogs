<body class="wikis">
  <div id="all">
    <h1>All Wikis</h1>
    <% unless current_user == nil %>
      <div class="col-md-6 pull-right">
        <%= link_to "Create New Wiki", new_wiki_path, class: 'btn btn-success' %>
    <% end %>
    </div>
    <% @wikis.each do |wiki| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
            <%= link_to wiki.title, wiki %>
          </h4>
        </div>
      </div>
    <% end %>
  </div>
</body>